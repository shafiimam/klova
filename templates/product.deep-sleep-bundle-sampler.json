    "new_faq_ym3cyh": {
      "type": "new-faq",
      "blocks": {
        "faq_zTdi4g": {
          "type": "FAQ",
          "settings": {
            "question": "MEET YOUR NEW BEDTIME BFF",
            "answer": "The Deep Sleep sample pack is curated to help you discover your perfect sleep solution. With a selection of our best sleep aid patches, you can find the optimal fit for your needs, ensuring a peaceful night's sleep and a refreshed awakening."
          }
        },
        "faq_tNCzii": {
          "type": "FAQ",
          "settings": {
            "question": "HOW IT WORKS?",
            "answer": "Our Deep Sleep patches are infused with ingredients like melatonin, hops, and valerian root (depending on the patch) that are known for their sleep-inducing properties. Transdermal delivery ensures these ingredients are absorbed consistently throughout the night, promoting uninterrupted and quality sleep."
          }
        },
        "faq_cTnn4x": {
          "type": "FAQ",
          "settings": {
            "question": "HOW TO USE IT?",
            "answer": "For a serene slumber, simply peel the backing off the patch of your choice, apply it to a clean area of your skin like the inside of your wrist or shoulder, and let the patch do the rest. The ingredients will help lull you into a peaceful sleep, allowing you to wake up rejuvenated."
          }
        },
        "faq_ftXPXK": {
          "type": "FAQ",
          "settings": {
            "question": "WHAT’S IN IT?",
            "answer": "The Deep Sleep sample pack includes 4 patches each of our dedicated sleep aids: Original Sleep, Extra Strength, and Melatonin-Free patches. It's designed to help you identify the right level of support for your sleep needs."
          }
        },
        "faq_PAGPfP": {
          "type": "FAQ",
          "settings": {
            "question": "BETTER SLEEP GUARANTEE",
            "answer": "Absolutely. Our Better Sleep Guarantee covers the Deep Sleep patches, ensuring that your journey to better rest is risk-free. If you're not satisfied with the improvement in your sleep quality, Klova offers a 30 day money back guarantee, providing a full refund, because your rest is our priority."
          }
        }
      },
